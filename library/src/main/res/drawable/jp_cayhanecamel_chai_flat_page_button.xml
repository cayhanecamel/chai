<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/shadow">
        <shape>
            <solid android:color="#D3CEC7"/>
            <padding android:bottom="1px"/>
        </shape>
    </item>
    <item android:id="@+id/face">
        <shape>
            <stroke
                android:width="1px"
                android:color="#AEA8A3"/>
            <solid android:color="@color/jp_cayhanecamel_chai_bg_color_gy02"/>
        </shape>
    </item>

    <item android:id="@+id/state">
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="#FF82DEFF"/>
                </shape>
            </item>
            <item android:state_focused="true">
                <shape android:shape="rectangle">
                    <solid android:color="#4082DEFF"/>
                </shape>
            </item>
        </selector>
    </item>

</layer-list>